  "/api/orders": "/orders",
  "/api/users": "/users",
  "/api/products/:id": "/products/:id",
  "/api/categories/:id": "/categories/:id",
  "/api/orders/:id": "/orders/:id",
  "/api/users/:id": "/users/:id",
  "/api/products?*": "/products?:1",
